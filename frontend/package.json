{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write src/**/*.ts{,x}",
    "check:type": "tsc --project tsconfig.json -pretty --noEmit",
    "lint:check": "eslint . --ext=js,jsx,ts,tsx;  exit 0",
    "lint:fix": "eslint . --ext=js,jsx,ts,tsx --fix;  exit 0",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && yarn"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged && yarn check:type"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --cache"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
